<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.farmstory.dao.ArticleDAO">
    <insert id="insertArticle">
        INSERT INTO `board_article` set `title`=#{title}, `content`=#{content}, `file`=#{file}, `uid`=#{uid}, `regip`=#{regip}, `rdate`=#{rdate};
    </insert>
    <select id="selectArticle"></select>
    <select id="selectArticles" resultType="kr.co.farmstory.vo.ArticleVO">
        SELECT * FROM `board_article` WHERE `cate`=#{cate};
    </select>
    <update id="updateArticle"></update>
    <delete id="deleteArticle"></delete>

</mapper>